<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fooood">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_fastfood_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_fastfood_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fooood">
        <activity android:name=".view.menu.MenuActivity" />
        <activity android:name=".view.search.SearchActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>